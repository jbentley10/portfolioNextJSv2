{
  "name": "app-template-nextjs",
  "version": "0.1.1",
  "private": true,
  "main": "index.js",
  "directories": {
    "src": "."
  },
  "scripts": {
    "build": "PORT=${PORT} next build",
    "gcp-build": "NODE_ENV='production' PORT=${PORT} next build",
    "dev": "NODE_ENV='development' node server.js",
    "deploy": "yarn run deploy-staging",
    "deploy-staging": "gcloud app deploy app.staging.yaml --project MYSITE-staging",
    "deploy-production": "gcloud app deploy app.yaml --project MYSITE",
    "start": "NODE_ENV='production' PORT=${PORT} node server.js",
    "storybook": "start-storybook -s ./ -p 6006 -c .storybook",
    "build-storybook": "build-storybook -c .storybook -o storybook-static && cp -r ./public storybook-static",
    "deploy-storybook": "sh ./deploy_storybook.sh",
    "app": "gulp scaffold:app",
    "component": "gulp scaffold:component --name ",
    "context": "gulp scaffold:context --name ",
    "api": "gulp scaffold:api --name ",
    "page": "gulp scaffold:page --name ",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@layerframers/whereami": "^1.1.6",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "@zeit/next-workers": "1.0.1-canary.1",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link-prismic": "^1.0.4",
    "compression": "^1.7.4",
    "d3-ease": "^1.0.6",
    "debounce": "^1.2.0",
    "express": "^4.17.1",
    "framer-motion": "^1.6.15",
    "graphql-tag": "^2.10.1",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-pngquant": "^8.0.0",
    "imagemin-svgo": "^7.0.0",
    "isomorphic-unfetch": "^3.0.0",
    "next": "^9.0.5",
    "next-compose-plugins": "^2.2.0",
    "next-fonts": "^0.18.0",
    "next-images": "^1.2.0",
    "next-optimized-images": "^2.5.3",
    "next-react-svg": "^1.1.0",
    "node-sass": "^4.12.0",
    "prismic-dom": "^2.1.0",
    "react": "^16.9.0",
    "react-cookie": "^4.0.1",
    "react-device-detect": "^1.9.10",
